{
    "name": "edgarcnp-dev-leptos",
    "version": "1.4.4",
    "private": true,
    "scripts": {
        "build:tailwind": "tailwindcss -i ./styles/tailwind.css -o ./pkg/edgarcnp-dev.css --minify",
        "dev:tailwind": "tailwindcss -i ./styles/tailwind.css -o ./pkg/edgarcnp-dev.css --watch",
        "build": "npm run build:tailwind && cargo leptos build",
        "dev": "npm run dev:tailwind & cargo leptos watch"
    },
    "devDependencies": {
        "tailwindcss": "^4.1.9",
        "@tailwindcss/postcss": "^4.1.9",
        "postcss": "^8.5.0",
        "tw-animate-css": "1.4.0"
    }
}
